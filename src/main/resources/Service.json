{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "kynexus",
    "annotations": {
      "description": "This template is for the kynexus app",
      "tags": "rest,java"
    }
  },
  "objects": [
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "kynexus",
        "labels": {
    			"app": "kynexus",
    			"com.gnoxy.label": "service_value",
		    	"com.gnoxy.common-label": "common-label-value"
        }
      },
      "spec": {
        "ports": [
          {
            "name": "8080-tcp",
            "protocol": "TCP",
            "port": 8080,
            "targetPort": 8080
          }
        ],
        "selector": {
          "deploymentconfig": "kynexus"
        },
        "type": "ClusterIP",
        "sessionAffinity": "None"
      },
      "status": {
        "loadBalancer": {}
      }
    }
  ],
  "labels": {
    "template": "application-template-kynexus"
  }
}